@using PustokProje.Models;
@model Settings;
@{
    ViewData["Title"] = "Edit";
}

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <form method="post" asp-action="edit">
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input type="text" class="form-control" asp-for="Key" placeholder="Enter key">
                    <span asp-validation-for="Key"></span>

                    <label asp-for="Value">Value</label>
                    <input type="text" class="form-control" asp-for="Value" placeholder="Enter value">
                    <span asp-validation-for="Value"></span>
                </div>



                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>
@section Scripts{
    @await Html.PartialAsync("_ValidationScriptPartial")
}

